{
  "name": "deadman-crypto-js",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript cryptography library for operations within deadman.wtf",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": "/dist",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "publish-lib": "npm run build && npm publish",
    "publish-beta": "npm run build && npm publish --tag beta",
    "publish-dryrun": "npm run build && npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadman-wtf/deadman-crypto-js.git"
  },
  "keywords": [
    "crypto",
    "cryptography",
    "typescript",
    "pvss",
    "dleq",
    "zkp"
  ],
  "author": "zaiteki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deadman-wtf/deadman-crypto-ts/issues"
  },
  "homepage": "https://github.com/deadman-wtf/deadman-crypto-ts#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.4.0"
  },
  "jestMatch": {
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
